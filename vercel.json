{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "installCommand": "npm install --legacy-peer-deps"
}