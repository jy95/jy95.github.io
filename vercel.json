{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "index, follow"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=600"
        }
      ]
    }
  ]
}